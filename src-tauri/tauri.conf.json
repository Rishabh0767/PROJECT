{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Postman",
  "version": "0.1.0",
  "identifier": "com.infoorigin.postman",
  "build": {
  "beforeDevCommand": "cd postman-clone && npm run dev",
  "beforeBuildCommand": "cd postman-clone && npm run build",
  "devUrl": "http://localhost:5173",
  "frontendDist": "../postman-clone/dist"
},
  "app": {
    "windows": [
      {
        "title": "Postman",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
